@model OregonNexus.Broker.Web.Models.SettingsViewModel;
@{
    // Copyright: 2023 Education Nexus Oregon
    // Author: Makoa Jacobsen, makoa@makoajacobsen.com
    Layout = "_SettingsLayout";
}
@if(Model is not null)
{
<div>
  <h2 class="text-base font-semibold leading-7 text-gray-900">Connectors</h2>
  <p class="mt-1 text-sm leading-6 text-gray-500">List of loaded connectors.</p>

  <ul role="list" class="mt-6 divide-y divide-gray-100 border-t border-gray-200 text-sm leading-6">
    @foreach(var connector in Model.ConnectorTypes)
    {
    <li class="flex justify-between gap-x-6 py-6">
      <div class="font-medium text-gray-900">@connector.AssemblyQualifiedName</div>
    </li>
    }
  </ul>
</div>
}